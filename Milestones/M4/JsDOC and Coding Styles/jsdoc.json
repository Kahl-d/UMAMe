{
    "source": {
      "include": ["src"], 
      "includePattern": ".+\\.(js|jsx)$", 
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"], 
    "opts": {
      "encoding": "utf8",
      "destination": "./docs", 
      "recurse": true,
      "verbose": true,
      "template": "node_modules/docdash"
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    }
  }
  